// ==UserScript==
// @name         ByteBreakerBypasser
// @namespace    http://tampermonkey.net/
// @version      1.0
// @description  Redirects loot-link URLs to KingExploits
// @match        *://loot-link.com/s?*
// @run-at       document-start
// @grant        none
// ==/UserScript==

(function() {
    'use strict';
    window.location.href = "https://www.kingexploits.com/your-key";
})();
